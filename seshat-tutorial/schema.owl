@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix doc: <http://localhost:6363/nuseshat/document/> .
@prefix terminus: <http://terminusdb.com/schema/terminus#> .
@prefix scm: <http://localhost:6363/nuseshat/schema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix xdd: <http://terminusdb.com/schema/xdd#> .
@prefix vio: <http://terminusdb.com/schema/vio#> .
@prefix tcs: <http://terminusdb.com/schema/tcs#> .
@prefix tbs: <http://terminusdb.com/schema/tbs#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://localhost:6363/nuseshat>
  a owl:Ontology ;
  rdfs:comment "This is the seshat that we made in Dublin on 15/01/2020"@en ;
  rdfs:label "Seshat Workshop"@en .

scm:CitedWork
  a owl:Class ;
  rdfs:label "Cited Work"@en ;
  rdfs:subClassOf tcs:Document .

scm:Confidence
  a owl:Class ;
  rdfs:comment "Tags that can be added to annotations to indicate confidence in the value of some piece data"@en ;
  rdfs:label "Content Tags"@en ;
  rdfs:subClassOf tcs:Enumerated ;
  owl:oneOf ( scm:inferred scm:disputed scm:uncertain ) .

scm:Conflict
  a owl:Class ;
  rdfs:comment "A conflict between societies"@en ;
  rdfs:label "Conflict"@en ;
  rdfs:subClassOf tcs:Document .

scm:EpistemicState
  a owl:Class ;
  rdfs:comment "The epistemic state of some variable"@en ;
  rdfs:label "Epistemic State"@en ;
  rdfs:subClassOf tcs:Enumerated ;
  owl:oneOf ( scm:unknown scm:absent scm:present ) .

scm:EpistemicStateValue
  a owl:Class ;
  rdfs:label "Epistemic State"@en ;
  rdfs:subClassOf scm:PropertyValue .

scm:Metals
  a owl:Class ;
  rdfs:comment "Dealing with minerals"@en ;
  rdfs:label "Metals"@en ;
  rdfs:subClassOf scm:Minerals .

scm:Minerals
  a owl:Class ;
  rdfs:comment "Dealing with minerals"@en ;
  rdfs:label "Minerals"@en .

scm:NGA
  a owl:Class ;
  rdfs:comment "Natural Geographic Region"@en ;
  rdfs:label "NGA"@en ;
  rdfs:subClassOf tcs:Document .

scm:Note
  a owl:Class ;
  rdfs:comment "Editorial Note on the value"@en ;
  rdfs:label "A Note on a value"@en .

scm:PoliticalAuthority
  a owl:Class ;
  rdfs:comment "Political Authority - a polity or quasi-polity"@en ;
  rdfs:label "PoliticalyAuthority"@en ;
  rdfs:subClassOf tcs:Document .

scm:Polity
  a owl:Class ;
  rdfs:label "Polity"@en ;
  rdfs:subClassOf scm:PoliticalAuthority ,
                  tcs:Document .

scm:PresenceOfIronValue
  a owl:Class ;
  rdfs:label "Presence of Iron"@en ;
  rdfs:subClassOf scm:EpistemicStateValue ,
                  scm:Metals .

scm:PropertyValue
  a owl:Class .

scm:absent
  a scm:EpistemicState ;
  rdfs:comment "The feature was absent in this historical context"@en ;
  rdfs:label "Absent"@en .

scm:citation
  a owl:ObjectProperty ;
  rdfs:domain scm:Note ;
  rdfs:label "Cited Work"@en ;
  rdfs:range scm:CitedWork .

scm:confidence
  a owl:ObjectProperty ;
  rdfs:domain scm:PropertyValue ;
  rdfs:range scm:Confidence .

scm:disputed
  a scm:Confidence ;
  rdfs:comment "The evidence is disputed - some believe this data to be incorrect"@en ;
  rdfs:label "Disputed"@en .

scm:end
  a owl:DatatypeProperty ;
  rdfs:domain scm:PropertyValue ;
  rdfs:range xdd:gYearRange .

scm:epistemicState
  a owl:ObjectProperty ;
  rdfs:domain scm:EpistemicStateValue ;
  rdfs:label "Epistemic State"@en ;
  rdfs:range scm:EpistemicState .

scm:inferred
  a scm:Confidence ;
  rdfs:comment "The value has been logically inferred from other evidence"@en ;
  rdfs:label "Inferred"@en .

scm:notes
  a owl:ObjectProperty ;
  rdfs:domain scm:PropertyValue ;
  rdfs:range scm:Note .

scm:predecessor
  a owl:ObjectProperty ;
  rdfs:domain scm:PoliticalAuthority ;
  rdfs:label "Predecessor"@en ;
  rdfs:range scm:PoliticalAuthority .

scm:presence_of_iron
  a owl:ObjectProperty ;
  rdfs:domain scm:NGA ;
  rdfs:label "Presence of Iron"@en ;
  rdfs:range scm:PresenceOfIronValue .

scm:present
  a scm:EpistemicState ;
  rdfs:comment "The feature was present in this historical context"@en ;
  rdfs:label "Present"@en .

scm:quotation
  a owl:DatatypeProperty ;
  rdfs:domain scm:Note ;
  rdfs:label "Quotation"@en ;
  rdfs:range xsd:string .

scm:remote_url
  a owl:DatatypeProperty ;
  rdfs:domain scm:CitedWork ;
  rdfs:range xsd:anyURI .

scm:start
  a owl:DatatypeProperty ;
  rdfs:domain scm:PropertyValue ;
  rdfs:range xdd:gYearRange .

scm:uncertain
  a scm:Confidence ;
  rdfs:comment "The evidence has a high degree of uncertainty"@en ;
  rdfs:label "Uncertain"@en .

scm:unknown
  a scm:EpistemicState ;
  rdfs:comment "A known unknown - it can be said with a high degree of confidence that it is not known whether the feature was present or absent in the context."@en ;
  rdfs:label "Unknown"@en .

tcs:ClassTag
  a owl:Class ;
  rdfs:comment "Tags that can be added to classes to add meta information"@en ;
  rdfs:label "Class Tags"@en ;
  rdfs:subClassOf tcs:Enumerated ;
  owl:oneOf ( tcs:abstract ) .

tcs:Enumerated
  a owl:Class ;
  tcs:tag tcs:abstract ;
  rdfs:comment "A class representing enumerated types / choice sets"@en ;
  rdfs:label "Enumerated Set"@en .

tcs:abstract
  a tcs:ClassTag ;
  rdfs:comment "Indicates that the class is abstract - purely a logical construct, no base instantiations exist"@en ;
  rdfs:label "Abstract"@en .

xdd:coordinate
  a rdfs:Datatype ;
  rdfs:comment "A longitude / latitude pair making up a coordinate."@en ;
  rdfs:label "Coordinate"@en .

xdd:coordinatePolygon
  a rdfs:Datatype ;
  rdfs:comment "A JSON list of coordinates forming a closed polygon."@en ;
  rdfs:label "Coordinate Polygon"@en .

xdd:coordinatePolyline
  a rdfs:Datatype ;
  rdfs:comment "A JSON list of coordinates."@en ;
  rdfs:label "Coordinate Polyline"@en .

xdd:dateRange
  a rdfs:Datatype ;
  rdfs:comment "A date (YYYY-MM-DD) or an uncertain date range [YYYY-MM-DD1,YYYY-MM-DD2]. Enables uncertainty to be encoded directly in the data"@en ;
  rdfs:label "Date Range"@en .

xdd:decimalRange
  a rdfs:Datatype ;
  rdfs:comment "Either a decimal value (e.g. 23.34) or an uncertain range of decimal values (e.g.[23.4, 4.143]. Enables uncertainty to be encoded directly in the data"@en ;
  rdfs:label "Decimal Range"@en .